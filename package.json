{
	"name": "antarys",
	"version": "0.1.0",
	"description": "High-performance Node.js client for Antarys vector database with HTTP/2, connection pooling, and intelligent caching",
	"main": "dist/index.js",
	"module": "dist/index.mjs",
	"types": "dist/index.d.ts",
	"exports": {
		".": {
			"import": "./dist/index.mjs",
			"require": "./dist/index.js",
			"types": "./dist/index.d.ts"
		},
		"./worker": {
			"import": "./dist/worker.mjs",
			"require": "./dist/worker.js"
		}
	},
	"files": [
		"dist/index.js",
		"dist/index.mjs",
		"dist/index.d.ts",
		"dist/index.d.ts.map",
		"dist/worker.js",
		"dist/worker.mjs",
		"dist/worker.d.ts",
		"README.md",
		"LICENSE",
		"CHANGELOG.md"
	],
	"scripts": {
		"dev": "tsx examples/basic.ts",
		"dev:test": "tsx simple-test.ts",
		"build": "npm run clean && npm run build:cjs && npm run build:esm && npm run build:bundle",
		"build:cjs": "tsc -p tsconfig.json",
		"build:esm": "tsc -p tsconfig.esm.json",
		"build:bundle": "npm run copy:main && npm run copy:worker && npm run create:esm",
		"clean": "rm -rf dist dist-esm dist-temp",
		"copy:main": "cp dist-temp/src/index.js dist/index.js && cp dist/src/index.d.ts dist/index.d.ts && cp dist/src/index.d.ts.map dist/index.d.ts.map",
		"copy:worker": "cp dist-temp/src/worker.js dist/worker.js && cp dist/src/worker.d.ts dist/worker.d.ts",
		"create:esm": "cp dist/index.js dist/index.mjs && cp dist/worker.js dist/worker.mjs",
		"lint": "eslint src/**/*.ts shared/**/*.ts",
		"lint:fix": "eslint src/**/*.ts shared/**/*.ts --fix",
		"format": "prettier --write \"src/**/*.ts\" \"shared/**/*.ts\"",
		"typecheck": "tsc --noEmit",
		"validate": "npm run lint && npm run typecheck",
		"prepack": "npm run build",
		"prepublishOnly": "npm run validate && npm run test && npm run build",
		"version:patch": "npm version patch",
		"version:minor": "npm version minor",
		"version:major": "npm version major",
		"release": "npm run version:patch && npm publish",
		"release:beta": "npm version prerelease --preid=beta && npm publish --tag beta"
	},
	"keywords": [
		"vector-database",
		"machine-learning",
		"embeddings",
		"similarity-search",
		"ai",
		"http2",
		"performance",
		"typescript",
		"nodejs",
		"antarys",
		"semantic-search",
		"vector-similarity",
		"hnsw",
		"approximate-nearest-neighbors"
	],
	"author": {
		"name": "Antarys",
		"email": "antarys.ai@gmail.com",
		"url": "https://antarys.ai"
	},
	"license": "MIT",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/antarys-ai/antarys-node.git"
	},
	"bugs": {
		"url": "https://github.com/antarys-ai/antarys-node/issues"
	},
	"homepage": "https://github.com/antarys-ai/antarys-node#readme",
	"engines": {
		"node": ">=18.0.0",
		"npm": ">=8.0.0"
	},
	"dependencies": {
		"lru-cache": "^11.1.0"
	},
	"devDependencies": {
		"@types/jest": "^30.0.0",
		"@types/node": "^24.0.4",
		"@typescript-eslint/eslint-plugin": "^8.35.0",
		"@typescript-eslint/parser": "^8.35.0",
		"eslint": "^9.29.0",
		"jest": "^29.7.0",
		"prettier": "^3.6.1",
		"ts-jest": "^29.4.0",
		"tsx": "^4.20.3",
		"typedoc": "^0.28.5",
		"typescript": "^5.8.3"
	},
	"peerDependencies": {
		"typescript": ">=4.5.0"
	},
	"publishConfig": {
		"access": "public",
		"registry": "https://registry.npmjs.org/"
	},
	"type": "module"
}
